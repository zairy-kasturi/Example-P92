#include <iostream>
#include <string>
using namespace std;

void calMulAndDiv(int num1, int num2, int &mul, int &div);

void main()
{
	int num1, num2;
	int mul = 0, div = 0;

	cout << "plz enter two inputs  : ";
	cin >> num1 >> num2;

	calMulAndDiv(num1, num2, mul, div);
	cout << num1 << " multiplied by "<<num2<< " is " << mul<<endl;
	cout << num1 << " divided by " << num2 << " is " << div;
}

void calMulAndDiv(int num1, int num2, int &mul, int &div)
{
	mul = num1 * num2;
	div = num1 / num2;
}
